{
    "name": "dayonyz/team-lead-t-70",
    "type": "project",
    "description": "Team-lead Terminator T-70",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "phpunit/phpunit": "^10.1",
        "vlucas/phpdotenv": "^5.6",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Src\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
